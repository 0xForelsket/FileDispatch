{
  "$schema": "https://schema.tauri.app/config/2",
  "identifier": "default",
  "description": "Default capabilities for File Dispatch.",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "notification:default",
    "dialog:default",
    "fs:default",
    "fs:allow-write-text-file",
    "fs:allow-read-text-file",
    "shell:default",
    "store:default",
    "log:default",
    "autostart:default"
  ]
}
